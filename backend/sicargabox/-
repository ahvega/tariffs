  schemas:
    Alerta:
      type: object
      properties:
        envio:
          type: integer
        cliente:
          type: integer
        tipo_alerta:
          $ref: '#/components/schemas/TipoAlertaEnum'
        mensaje:
          type: string
        fecha_envio:
          type: string
          format: date-time
          readOnly: true
        estado:
          $ref: '#/components/schemas/AlertaEstadoEnum'
      required:
      - cliente
      - envio
      - estado
      - fecha_envio
      - mensaje
      - tipo_alerta
    AlertaEstadoEnum:
      enum:
      - Enviado
      - Error
      type: string
      description: |-
        * `Enviado` - Enviado
        * `Error` - Error
    AlertaRequest:
      type: object
      properties:
        envio:
          type: integer
        cliente:
          type: integer
        tipo_alerta:
          $ref: '#/components/schemas/TipoAlertaEnum'
        mensaje:
          type: string
          minLength: 1
        estado:
          $ref: '#/components/schemas/AlertaEstadoEnum'
      required:
      - cliente
      - envio
      - estado
      - mensaje
      - tipo_alerta
    Articulo:
      type: object
      properties:
        cotizacion:
          type: integer
        valor_articulo:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
        largo:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        ancho:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        alto:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        peso:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        impuesto_dai:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          nullable: true
        impuesto_isc:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          nullable: true
        impuesto_ispc:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          nullable: true
        impuesto_isv:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          nullable: true
        impuesto_total:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          nullable: true
      required:
      - alto
      - ancho
      - cotizacion
      - largo
      - peso
      - valor_articulo
    ArticuloAPI:
      type: object
      description: |-
        Serializer for Articulo (Shipment Item).

        Represents an individual item within a quotation, including its physical
        dimensions, weight, value, and calculated tax information.

        Fields:
            id (int): Unique identifier
            cotizacion (int): Quotation ID (foreign key to Cotizacion)
            valor_articulo (decimal): Item value in local currency
            largo (decimal): Length in cm
            ancho (decimal): Width in cm
            alto (decimal): Height in cm
            peso (decimal): Actual weight in kg
            peso_volumetrico (decimal): Volumetric weight in kg (read-only, calculated)
            peso_a_usar (decimal): Weight to use for shipping calculation (read-only, max of peso and peso_volumetrico)
            partida_arancelaria (int): Tariff item ID (foreign key to PartidaArancelaria)
            impuesto_dai (decimal): DAI tax amount (read-only, auto-calculated)
            impuesto_isc (decimal): ISC tax amount (read-only, auto-calculated)
            impuesto_ispc (decimal): ISPC tax amount (read-only, auto-calculated)
            impuesto_isv (decimal): ISV tax amount (read-only, auto-calculated)
            impuesto_total (decimal): Total tax amount (read-only, auto-calculated)

        Note:
            All tax fields are automatically calculated upon creation based on the
            associated partida arancelaria's tax rates.
      properties:
        id:
          type: integer
          readOnly: true
        cotizacion:
          type: integer
        valor_articulo:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
        largo:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        ancho:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        alto:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        peso:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        peso_volumetrico:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          readOnly: true
        peso_a_usar:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          readOnly: true
        partida_arancelaria:
          type: integer
        impuesto_dai:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          readOnly: true
          nullable: true
        impuesto_isc:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          readOnly: true
          nullable: true
        impuesto_ispc:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          readOnly: true
          nullable: true
        impuesto_isv:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          readOnly: true
          nullable: true
        impuesto_total:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
          readOnly: true
          nullable: true
      required:
      - alto
      - ancho
      - cotizacion
      - id
      - impuesto_dai
      - impuesto_isc
      - impuesto_ispc
      - impuesto_isv
      - impuesto_total
      - largo
      - partida_arancelaria
      - peso
      - peso_a_usar
      - peso_volumetrico
      - valor_articulo
    ArticuloAPIRequest:
      type: object
      description: |-
        Serializer for Articulo (Shipment Item).

        Represents an individual item within a quotation, including its physical
        dimensions, weight, value, and calculated tax information.

        Fields:
            id (int): Unique identifier
            cotizacion (int): Quotation ID (foreign key to Cotizacion)
            valor_articulo (decimal): Item value in local currency
            largo (decimal): Length in cm
            ancho (decimal): Width in cm
            alto (decimal): Height in cm
            peso (decimal): Actual weight in kg
            peso_volumetrico (decimal): Volumetric weight in kg (read-only, calculated)
            peso_a_usar (decimal): Weight to use for shipping calculation (read-only, max of peso and peso_volumetrico)
            partida_arancelaria (int): Tariff item ID (foreign key to PartidaArancelaria)
            impuesto_dai (decimal): DAI tax amount (read-only, auto-calculated)
            impuesto_isc (decimal): ISC tax amount (read-only, auto-calculated)
            impuesto_ispc (decimal): ISPC tax amount (read-only, auto-calculated)
            impuesto_isv (decimal): ISV tax amount (read-only, auto-calculated)
            impuesto_total (decimal): Total tax amount (read-only, auto-calculated)

        Note:
            All tax fields are automatically calculated upon creation based on the
            associated partida arancelaria's tax rates.
      properties:
        cotizacion:
          type: integer
        valor_articulo:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
        largo:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        ancho:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        alto:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        peso:
          type: string
          format: decimal
          pattern: ^-?\d{0,3}(?:\.\d{0,2})?$
        partida_arancelaria:
          type: integer
      required:
      - alto
      - ancho
      - cotizacion
      - largo
      - partida_arancelaria
      - peso
      - valor_articulo
