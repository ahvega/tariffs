    name: Proprietary
paths:
  /MiCasillero/api/v1/Alerta/:
    get:
      operationId: MiCasillero_api_v1_Alerta_list
      description: ViewSet for the Alerta class
      parameters:
      - name: ordering
        required: false
        in: query
        description: Which field to use when ordering the results.
        schema:
          type: string
      - name: page
        required: false
        in: query
        description: A page number within the paginated result set.
        schema:
          type: integer
      - name: search
        required: false
        in: query
        description: A search term.
        schema:
          type: string
      tags:
      - MiCasillero
      security:
      - cookieAuth: []
      - basicAuth: []
